[{"h2": "\u7b80\u4ecb", "content": "\u536b\u8f89\u5e02\u4f4d\u4e8e\u6cb3\u5357\u7701\u5317\u90e8\uff0c\u662f\u65b0\u4e61\u5e02\u4e0b\u8f96\u7684\u4e00\u4e2a\u53bf\u7ea7\u5e02\uff0c\u592a\u884c\u5c71\u4e1c\u9e93\u3002\u9762\u79ef882\u5e73\u65b9\u5343\u7c73\u3002"}, {"h2": "\u5386\u53f2", "content": "\u897f\u6c49\u7f6e\u6c72\u53bf\u3002\u5317\u5468\u65f6\u8bbe\u536b\u5dde\u3002\u968b\u4ee3\u5f00\u7687\u516d\u5e74\uff08586\u5e74\uff09\u53bf\u6cbb\u7531\u6c72\u57ce\u6751\u79fb\u81f3\u4eca\u5740\u3002\u5510\u671d\u65f6\uff0c\u6c72\u53bf\u5c5e\u536b\u5dde\u3002\u5143\u671d\u65f6\u5c5e\u536b\u8f89\u8def\u3002\u660e\u4ee3\u3001\u6e05\u4ee3\u5c5e\u536b\u8f89\u5e9c\uff0c1948\u5e7411\u67087\u65e5\uff0c\u4e2d\u5171\u653f\u6743\u5360\u9886\u6c72\u53bf\u5168\u5883\uff0c\u65b0\u653f\u5e9c\u5206\u5272\u90e8\u5206\u53bf\u5883\u8bbe\u536b\u8f89\u5e02\uff0c1949\u5e74\u5e76\u56de\u6c72\u53bf\u30021988\u5e74\u64a4\u6c72\u53bf\uff0c\u8bbe\u7acb\u536b\u8f89\u5e02\u30022021\u5e747\u6708\uff0c\u6cb3\u5357\u906d\u9047\u7279\u5927\u66b4\u96e8\u88ad\u51fb\uff0c\u5171\u4ea7\u4e3b\u4e49\u6e20\u4e0e\u536b\u6cb3\u6d2a\u6c34\u6f2b\u6ea2\uff0c\u5bfc\u81f4\u536b\u8f89\u5e02\u533a\u5168\u57ce\u88ab\u6d2a\u6c34\u56f4\u56f0\uff0c\u6c34\u4f4d\u6700\u6df1\u5904\u8fbe1.6\u7c73\u4ee5\u4e0a\uff0c\u53d7\u707e\u4e25\u91cd\u3002"}, {"h2": "\u884c\u653f\u533a\u5212", "content": "\u536b\u8f89\u5e02\u4e0b\u8f967\u4e2a\u9547\u30016\u4e2a\u4e61\uff1a\u6c72\u6c34\u9547\u3001\u200b\u592a\u516c\u9547\u3001\u200b\u5b59\u674f\u6751\u9547\u3001\u200b\u540e\u6cb3\u9547\u3001\u200b\u674e\u6e90\u5c6f\u9547\u3001\u200b\u5510\u5e84\u9547\u3001\u200b\u4e0a\u4e50\u6751\u9547\u3001\u200b\u72ee\u8c79\u5934\u4e61\u3001\u200b\u5b89\u90fd\u4e61\u3001\u200b\u987f\u574a\u5e97\u4e61\u3001\u200b\u67f3\u5e84\u4e61\u3001\u200b\u5e9e\u5be8\u4e61\u3001\u200b\u57ce\u90ca\u4e61\u3001\u200b\u539f\u79cd\u573a\u3001\u200b\u4e1c\u98ce\u519c\u573a\u3001\u200b\u82d7\u5703\u573a\u3001\u200b\u519c\u79d1\u6240\u548c\u4e94\u56db\u519c\u573a\u3002"}, {"h2": "\u7ecf\u6d4e", "content": "\u6c34\u6ce5\u5382\u5728\u5de5\u4e1a\u603b\u4ea7\u503c\u4e2d\u5360\u8f83\u5927\u6bd4\u4f8b\u3002\u864e\u5934\u724c\u94a2\u94b3\u8f83\u6709\u540d\u3002\u4e0a\u4e50\u6751\u7a7a\u5fc3\u6302\u9762\u4e3a\u540d\u4ea7\u3002"}, {"h2": "\u4ea4\u901a", "content": "\u4eac\u5e7f\u94c1\u8def\u3001\u4eac\u6e2f\u6fb3\u9ad8\u901f\u516c\u8def\u7eb5\u8d2f\u5357\u5317\u3002"}, {"h2": "\u536b\u8f89\u5e02\u5e02\u957f", "content": "\u4e0b\u8868\u5217\u51fa\u5386\u4efb\u536b\u8f89\u5e02\u5e02\u957f\uff0c\u53ca\u5176\u524d\u8eab\u6c72\u53bf\u53bf\u957f\uff1a"}]